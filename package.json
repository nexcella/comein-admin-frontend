{
  "name": "@nexcella/comein-web",
  "version": "1.2.0-rc.1",
  "description": "Web application for ComeIn admin panel",
  "contributors": [
    {
      "name": "Andrey Shabanov",
      "email": "a.shabanov.develop@gmail.com"
    }
  ],
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --mode production",
    "test": "tsc -p .",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@nexcella/logger": "1.0.0",
    "@types/yup": "0.26.33",
    "axios": "0.19.2",
    "formik": "2.1.4",
    "i18next": "19.3.3",
    "i18next-browser-languagedetector": "4.0.2",
    "localforage": "1.7.3",
    "mobx": "5.15.4",
    "mobx-react": "6.1.8",
    "mobx-sync": "2.1.3",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-google-login": "5.1.2",
    "react-i18next": "11.3.4",
    "react-router-dom": "5.1.2",
    "typescript": "3.8.3",
    "webpack": "4.42.0",
    "yup": "0.28.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@types/react": "16.9.25",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "5.1.3",
    "@types/webpack": "4.41.8",
    "@types/webpack-dev-server": "3.10.1",
    "astroturf": "0.10.2",
    "dotenv": "8.2.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.1",
    "sass-loader": "8.0.2",
    "semantic-release": "17.0.4",
    "semantic-release-cli": "5.2.3",
    "style-loader": "1.1.3",
    "terser-webpack-plugin": "2.3.5",
    "ts-loader": "6.2.1",
    "url-loader": "4.0.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a-shabanov/comein-admin-frontend.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      {
        "name": "release/([0-9]+)\\.([0-9]+)\\.([0-9]+)",
        "channel": "rc",
        "prerelease": "rc"
      },
      {
        "name": "develop",
        "prerelease": "beta"
      },
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version}"
        }
      ]
    ]
  }
}
